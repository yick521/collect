`id` int(11) NOT NULL AUTO_INCREMENT COMMENT '主键',
`job_code` varchar(64) DEFAULT NULL COMMENT '任务code',
`job_name` varchar(256) DEFAULT NULL COMMENT '任务名称',
`job_desc` varchar(256) DEFAULT NULL COMMENT '任务描述',
`dataset_name` varchar(256) DEFAULT NULL COMMENT '数据集名称',
`dataset_table_name` varchar(256) DEFAULT NULL COMMENT '数据集表名称，可视化数据集是是节点code',
`flow_id` int(11) DEFAULT NULL COMMENT '画布流程id 可视化数据集任务才有',
`level` varchar(10) DEFAULT NULL COMMENT '优先级 lower middle high',
`sync_type` int(11) DEFAULT NULL COMMENT '是否增量更新 1-全量 2-增量',
`sync_column` varchar(256) DEFAULT NULL COMMENT '增量更新字段',
`update_setting` varchar(256) DEFAULT NULL COMMENT '更新配置',
`creat_user_id` int(11) DEFAULT NULL COMMENT '创建者id',
`creat_time` datetime DEFAULT NULL COMMENT '创建时间',
`update_time` datetime DEFAULT NULL COMMENT '更新时间',
`last_update_time` datetime DEFAULT NULL COMMENT '最近更新时间',
`next_update_time` datetime DEFAULT NULL COMMENT '下次更新时间',
`update_type` int(11) DEFAULT NULL COMMENT '更新方式:1-手动更新 2-自动更新 ',
`update_status` varchar(64) DEFAULT NULL COMMENT '更新成功: execution/success/fail ',
`job_type` int(11) DEFAULT NULL COMMENT '任务类型:1-数据集 2-可视化',
`job_status` int(11) DEFAULT NULL COMMENT '任务状态:1-启用 2-禁用 3-删除 4-不可用',
`job_json` text COMMENT '可视化数据集任务jobjson',
`db_type` varchar(64) DEFAULT NULL COMMENT '数据源类型:mysql/impala/可视化',
`update_corn` varchar(256) DEFAULT NULL COMMENT '更新时间的cron表达式',
`app_id` int(11) DEFAULT NULL COMMENT '应用id',
`product_id` varchar(64) DEFAULT NULL COMMENT '项目id',